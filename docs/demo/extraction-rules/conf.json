{
   "processType":"ETL",
   "originAppLocationCode":"testing_etl_with_extraction_rules",
   "etlRootDirectory":"/path/to/any/directory",
   "syncStageSchema":"etl_demo_stage_area",
   "srcConnInfo":{
      "dataBaseUserName":"",
      "dataBaseUserPassword":"",
      "connectionURI":"jdbc:mysql://localhost:3306/etl_demo_with_extraction_rules_src_db?autoReconnect=true&useSSL=false&allowPublicKeyRetrieval=true",
      "driveClassName":"com.mysql.cj.jdbc.Driver"
   },
   "dstConnInfo":{
      "dataBaseUserName":"",
      "dataBaseUserPassword":"",
      "connectionURI":"jdbc:mysql://localhost:3306/etl_demo_with_extraction_rules_dst_db?autoReconnect=true&useSSL=false&allowPublicKeyRetrieval=true",
      "driveClassName":"com.mysql.cj.jdbc.Driver"
   },
   "params":{
      "anexesOffices":"10002,10003"
   },
   "operations":[
      {
         "operationType":"ETL",
         "processingBatch":1000,
         "dstType":"db"
      }
   ],
   "etlItemConfiguration":[
      {
         "srcConf":{
            "tableName":"person",
            "auxExtractTable":[
               {
                  "tableName":"office",
                  "joinExtraCondition":"id in (@anexesOffices)"
               }
            ],
            "extraConditionForExtract":"not exists (select * from person_data where person_data.person_uuid = person.uuid)",
            "extraTableDataSource":[
               {
                  "tableName":"address",
                  "joinType":"INNER"
               }
            ]
         },
         "dstConf":[
            {
               "tableName":"person_data",
               "mapping":[
                  {
                     "dataSourceName":"person",
                     "srcField":"uuid",
                     "dstField":"person_uuid"
                  },
                  {
                     "dataSourceName":"person",
                     "srcField":"creation_date",
                     "dstField":"person_creation_date"
                  },
                  {
                     "dataSourceName":"address",
                     "srcField":"uuid",
                     "dstField":"address_uuid"
                  },
                  {
                     "dataSourceName":"address",
                     "srcField":"creation_date",
                     "dstField":"address_creation_date"
                  }
               ],
               "ignorableFields":[
                  "creation_date"
               ]
            }
         ]
      }
   ]
}